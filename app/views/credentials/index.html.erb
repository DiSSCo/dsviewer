<p id="notice"><%= notice %></p>

<h1>Credentials</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Url</th>
      <th>Server type</th>
      <th>In use</th>
      <th>Default</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @credentials.each do |credential| %>
      <tr>
        <td><%= credential.name %></td>
        <td><%= credential.description %></td>
        <td><%= credential.url %></td>
        <td><%= credential.server_type %></td>
        <td><%= credential.in_use %></td>
        <td><%= credential.default %></td>
        <td><%= link_to 'Show', credential %></td>
        <td><%= link_to 'Edit', edit_credential_path(credential) %></td>
        <td><%= link_to 'Destroy', credential, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Credential', new_credential_path %>
