<p id="notice"><%= notice %></p>

<h4>Digital Specimens List</h4>

<table  class="table table-striped table-bordered table-responsive">
  <thead>
    <tr>
      <th> Specimen</th>
      <th class="d-none d-lg-block"> MIDS Level</th>
      <th>Scientific Name</th>
      <th>Country</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @dsdata.each do |dsdatum| %>
      <tr>
        <td><%= dsdatum.id.split("/")[1] %></td>
        <td class="d-none d-lg-block"><%= dsdatum.midslevel %></td>
        <td><%= dsdatum.scientificName %></td>
        <td><%= dsdatum.country %></td>
        <td>
            <% @id=dsdatum.id.split("/")[1] %>
            <%= link_to 'Show', {:controller => 'dsdata',
              :action => 'show', :id =>@id}, :class => "btn btn-primary btn-xs" %> 
        </td>
        <td><%= link_to 'Edit', {:controller => 'dsdata',
              :action => 'edit', :id =>@id}, :class => "btn btn-success btn-xs" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

